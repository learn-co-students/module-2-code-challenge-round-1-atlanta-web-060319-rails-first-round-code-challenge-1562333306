<h2>All Heroines</h2>

<%= form_tag(heroines_path, :method => "get", id: "search-form") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search Powers" %>
  <%= submit_tag "Search" %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>

<%= link_to "Create New Heroine", new_heroine_path %>
